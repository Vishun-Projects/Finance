{
  "include": [
    "api/**/*.py",
    "tools/**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.next"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "extraPaths": [
        "tools",
        "tools/parsers",
        "api/parse-pdf-python",
        "api/parse-pdf-python/parsers",
        "api/parse-bank-statement-python",
        "api/parse-bank-statement-python/parsers",
        "api/parse-file-python",
        "api/parse-file-python/parsers"
      ]
    }
  ]
}

